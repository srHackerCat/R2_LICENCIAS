            <dataField fieldName="Comprobante" columns="15">
              <headerText>Adjunto</headerText>
            </dataField>
          </dataFields>
        </category>
        <category headerText="ESPECIFICACIONES" id="c3" wrap="true" floating="true" flow="NewColumn">
          <visibility>
