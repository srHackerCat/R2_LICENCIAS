  <businessRules>
    <rule id="r100" commandName="New" type="Sql" phase="Execute"><![CDATA[SET @IDEstado= 1
SET @EstadosEstado = (SELECT ESTADO FROM ESTADOS WHERE ID_ESTADO =@IDEstado)

SET @IDTipoTransaccion=1
SET @TheidTipoTransacciondescripcion = (SELECT DESCRIPCION FROM TIPO_TRANSACCION WHERE ID_TIPO_TRANSACCION =@IDTipoTransaccion)]]></rule>
  </businessRules>
