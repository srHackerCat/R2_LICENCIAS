            <dataField fieldName="UsuarioCrea" columns="20" />
            <dataField fieldName="FechaCrea" columns="20" />
            <dataField fieldName="UsuarioActualiza" columns="20" />
            <dataField fieldName="FechaActualiza" columns="20" />
          </dataFields>
        </category>
      </categories>
