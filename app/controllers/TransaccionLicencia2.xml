            <dataField fieldName="IDEntrega" aliasFieldName="EntregaDepositoNombreColaborador" />
            <dataField fieldName="EntregaDepositoFechaEntrega" columns="20" />
            <dataField fieldName="IDOcupacion" aliasFieldName="PermisoOcupacion" />
